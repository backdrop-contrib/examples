<?php
/**
 * @file
 * Installs, updates, and uninstalls cron_example.
 */

/**
 * Implements hook_install().
 *
 * @todo Remove this file once the cron_example.json file will be provided.
 */
function cron_example_install() {
  $config = config('cron_example');
  $config->set('cron_example_interval', 60*60);
  $config->save();
}
